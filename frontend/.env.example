# Application Version
VITE_VERSION="1.0.0"

# API Configuration
VITE_API_BASE_URL_DOMAIN=https://your-api-domain.com
VITE_API_BASE_URL=https://your-api-domain.com/api/
VITE_UAP_API_PREFIX='uap/v1/'
VITE_UAP_API_SERVICENOW_PREFIX=''

# Authentication Configuration
VITE_APP_AUTH_URL=https://your-auth-server.com/
VITE_APP_BASE_URL=https://your-app-domain.com
VITE_OIDC_CLIENT_ID=your-oidc-client-id
VITE_OIDC_CLIENT_SECRET=your-oidc-client-secret
VITE_DISABLE_PKCE=false

# Application Features
VITE_ENABLE_SIDEBARE=true
VITE_ENABLE_DEVLOGIN=true
VITE_ENABLE_STOREDEVTOOLS=true

# Content Evaluation Configuration
VITE_CONTENT_EVALUATION=your-content-evaluation-id
VITE_CONTENT_EVALUATION_REPORT_ID=your-report-id
VITE_SATISFIED_ANSWER_ID=your-satisfied-answer-id
VITE_NOT_SATISFIED_ANSWER_ID=your-not-satisfied-answer-id

# Authentication API Endpoints (for regular login)
# Used by the authService for API-based authentication
# VITE_API_BASE_URL is used as the base URL for auth endpoints:
# - POST ${VITE_API_BASE_URL}/auth/login
# - POST ${VITE_API_BASE_URL}/auth/refresh
# - POST ${VITE_API_BASE_URL}/auth/logout
# - GET ${VITE_API_BASE_URL}/auth/validate
